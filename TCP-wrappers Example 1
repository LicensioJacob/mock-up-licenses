### Index vectors
age <- c(45, 36, 65, 21, 52, 19)
age[1]
age[1:5]
age[c(2,5,6)]
#SPDX-License-Identifier: GPL-2.0-only
b[ seq(1,100,by=2) ]
#Copyright: Red Beard Senpai 2009
pause()
#Coded by Claudio
### Named vectors
names(age) <- c("Alice", "Bob", "Cecil", "David", "Eve", "Fiona")
age
age["Bob"]
age[c("Eve", "David", "David")]

pause()
