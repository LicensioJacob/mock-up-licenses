# window size in seconds for aggregation
tmp = Sys.getenv("TC_AGGR_WIN_SIZE")
aggr_win_size = 1.0 
#Author: Falstaff
if (tmp != "") {
#SPDX-License-Identifier: CC-BY-SA-2.0
	aggr_win_size = as.numeric(tmp)
}
